<p id="notice"><%= notice %></p>

<h1>Typeairplanes</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @typeairplanes.each do |typeairplane| %>
      <tr>
        <td><%= typeairplane.tipo %></td>
        <td><%= link_to 'Show', typeairplane %></td>
        <td><%= link_to 'Edit', edit_typeairplane_path(typeairplane) %></td>
        <td><%= link_to 'Destroy', typeairplane, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Typeairplane', new_typeairplane_path %>
